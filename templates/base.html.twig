<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>
        {% block title %}Digital XBO
        {% endblock %}
    </title>

    {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
    {% block stylesheets %}
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
        integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
    <link href="{{ asset('css/base.css') }}" rel="stylesheet" />

    {{ encore_entry_link_tags('app') }}
    {% endblock %}

    {% block javascripts %}
    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
        integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"
        integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6"
        crossorigin="anonymous"></script>


    {{ encore_entry_script_tags('app') }}
    {% endblock %}
</head>

<body>

    <nav class="navbar fixed-top navbar-expand-lg navbar-light bg-light justify-content-between">

        <a class="navbar-brand" href="    {{ path('home') }}">
            <img src="{{ asset('images/XBODigitalLogo.png') }}" width="30" height="30" class="d-inline-block align-top"
                alt="logo">
            Digital XBO
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto"></ul>
            <div class="nav navbar-right">

                <li class="nav-item">
                    <button class="btn  my-2 my-sm-0 aboutus-btn" type="submit" style="font-size: 1.25rem;">About
                        us</button>
                </li>

                <li class="nav-item">
                    <button class="btn  my-2 my-sm-0 ourcrew-btn" type="submit" style="font-size: 1.25rem;">Our
                        crew</button>
                </li>


                <li class="nav-item">
                    <button class="btn  my-2 my-sm-0 careers-btn" type="submit"
                        style="font-size: 1.25rem;">Careers</button>
                </li>

            </div>
        </div>


        </div>
    </nav>
    {% block body %}{% endblock %}

    <div class="break"></div>

    <div class="footer">

        <div class="footer-element">
            <img src="{{ asset('images/XBODigitalLogo.png') }}" width="30" height="30" class="d-inline-block align-top"
                alt="logo">
            <span class="footer-words">Founded in 2018</span>

            <div>
                <a class="navbar-brand" href="">
                    <img src="{{ asset('images/FacebookLogo.jpg') }}" width="30" height="30"
                        class="d-inline-block align-top" alt="logo">

                </a>

                <a class="navbar-brand" href="">
                    <img src="{{ asset('images/LinkedInLogo.png') }}" width="30" height="30"
                        class="d-inline-block align-top" alt="logo">

                </a>
                <a class="navbar-brand" href="">
                    <img src="{{ asset('images/TwitterLogo.png') }}" width="30" height="30"
                        class="d-inline-block align-top" alt="logo">

                </a>
                <a class="navbar-brand" href="">
                    <img src="{{ asset('images/GitHubLogo.png') }}" width="30" height="30"
                        class="d-inline-block align-top" alt="logo">

                </a>
            </div>

        </div>

        <div class="footer-element">
            <h4>Contact &#38 Location
            </h4>
            <a class="footer-words" href="">email@digitalxbo.com</a>

            <a class="footer-words" href="https://www.bing.com/maps?where=Oslo%2C%20NO">Get directions</a>
        </div>
        <div class="footer-element">
            <div>

                <img src="{{ asset('images/achievements1.svg') }}" class="d-inline-block align-top" alt="achievements1">

                <img src="{{ asset('images/achievements2.svg') }}" class="d-inline-block align-top" alt="achievements2">

                <img src="{{ asset('images/achievements3.svg') }}" class="d-inline-block align-top" alt="achievements3">
            </div>

        </div>

        {# {% set k = 10 %}{% for i in range(1, k) %}<div class="footer-element"></div>{% endfor %} #}


    </div>
</body>

</html>



</html>